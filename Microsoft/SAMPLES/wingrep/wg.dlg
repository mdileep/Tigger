    CONTROL "Recurse", ID_CHKRECURSE, "button", BS_AUTOCHECKBOX | WS_TABSTOP | WS_CHILD, 116, 88, 65, 8
    CONTROL "Case Sensitive", ID_CHKCASE, "button", BS_AUTOCHECKBOX | WS_TABSTOP | WS_CHILD, 8, 76, 65, 8
    CONTROL "Minimize on GO!", ID_CHKMINIMIZE, "button", BS_AUTOCHECKBOX | WS_TABSTOP | WS_CHILD, 116, 76, 65, 8
    CONTROL "GREP Files", ID_CHKGREP, "button", BS_AUTOCHECKBOX | WS_TABSTOP | WS_CHILD, 8, 88, 65, 8
    CONTROL "Ready to Rock!", ID_STATUS, "static", SS_LEFT | WS_CHILD, 4, 178, 240, 8
END


